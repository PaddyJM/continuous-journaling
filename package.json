{
  "name": "@rnr/starter-base",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start -c --ios",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "lint": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md}\" --config ./.prettierrc.json",
    "deploy:api:dev": "cd infrastructure/ && docker compose up -d && ENV=dev npx cdk synth && sam local start-api -p $(grep LOCAL_API_PORT ../.env | cut -d '=' -f2) -t ./cdk.out/ContinuousJournalingStack-dev.template.json --warm-containers EAGER --skip-pull-image && cd ../"
  },
  "dependencies": {
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^6.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "expo": "^51.0.8",
    "expo-linking": "~6.3.1",
    "expo-navigation-bar": "~3.0.4",
    "expo-router": "~3.5.14",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "lucide-react": "^0.381.0",
    "lucide-react-native": "^0.378.0",
    "nativewind": "^4.0.33",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.6",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/js": "^9.3.0",
    "@types/react": "~18.2.79",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.3.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.11.0"
  },
  "private": true
}
